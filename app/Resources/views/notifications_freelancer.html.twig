<li class="dropdown notifications-menu fixed">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
        <i class="fa fa-bell-o"></i>
        <span class="label label-warning">{{ v }}</span>
    </a>
    <ul class="dropdown-menu">
        <li class="header">You have {{ v }} notifications</li>
        <li>
            <!-- inner menu: contains the actual data -->
            <ul class="menu">
                {% for p in projetsNotif if p.satus == 1 %}
                    {% if p.satus == 1 %}
                        <li>
                            <a href="{{ path('freelancer_main_listencoursf_projet') }}">
                                <small>
                                    <small class="label pull-left bg-aqua-active">{{ p.email }}</small>
                                    <small class="label pull-left bg-black-active">vous confirme pour</small>
                                    <small class="label pull-left bg-yellow-active">{{ p.nom }}</small> 
                                </small>
                            </a>
                        </li>
                    {%  endif  %}
                    {% if p.satus == 2 %}
                        <li>
                            <a href="{{ path('freelancer_main_listencoursf_projet') }}">
                            <small>
                                    <small class="label pull-left bg-black-active">vous vous êtes payés par</small>
                                    <small class="label pull-left bg-gray-active">{{ p.email }}</small>
                                    <small class="label pull-left bg-black-active">au projet</small>
                                    <small class="label pull-left bg-yellow-active">{{ p.nom }}</small> 
                            </small>
                            </a>
                        </li>
                    {%  endif  %}
                {% endfor %}
            </ul>
        </li>
        <li class="footer"><a href="{{ path('freelancer_main_listencoursf_projet') }}">Voir tout</a></li>
    </ul>
</li>