{% extends "base.html.twig" %}
{% block body %}
 <form action="" method="POST">
            <table >
                <tr>
                    <td>{{form_label(Form.nom, "Nom: ")}}</td>
                    <td>{{form_widget(Form.nom)}}</td>
                </tr>
                <tr>
                    <td>{{form_label(Form.budget , "Budget: ")}}</td>
                    <td>{{form_widget(Form.budget )}}</td>
                </tr>
                <tr>
                    <td>{{form_label(Form.checkIn, "checkin: ")}}</td>
                    <td>{{form_widget(Form.checkIn)}}</td>
                </tr>
                <tr> 
                    <td> {{form_label(Form.checkOut, "chekout: ")}}</td>
                    <td>{{form_widget(Form.checkOut)}}</td>
                </tr>
                <tr> 
                    <td> {{form_label(Form.description, "description: ")}}</td>
                    <td>{{form_widget(Form.description)}}</td>
                </tr>
                <tr><td>{{form_widget(Form.valider)}}</td></tr>
                <tr><td>{{form_widget(Form.annuler)}}</td></tr>
                {{ form_rest(Form)}}
            </table>
        </form>
  {% endblock %}